{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": false,
    "noImplicitOverride": true,
    "jsx": "react",
    "useDefineForClassFields": true,
    "lib": ["dom", "es2022"]
  },
  "include": ["src"]
}
